      <characteristics>
        <characteristic characteristicId="d16a-97b5-407f-dcc5" name="Effects" value="Aids the wizard in divining the future. Once per game, he may add +2 to an initiative roll."/>
      </characteristics>
      <modifiers/>
    </profile>
